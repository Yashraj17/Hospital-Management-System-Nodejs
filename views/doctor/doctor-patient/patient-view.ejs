<%- include('../navbar') %>
<div class="container-fluid ps-0">
    <div class="row w-100 p-0">
        <div class="col-2" id="side" style="height:91vh;">
            <%- include('../side') %>
        </div>
        <div class="col-10">
            <h4 class="mt-3 border-start border-4 px-2">Your Total Patient List</h4>
            <table class="table mt-4">
                <thead class="table-dark">
                  <tr>
                      <th>Patient Name</th>
                      <th>Patient Picture</th>
                      <th>Symptoms</th>
                      <th>Mobile</th>
                      <th>Address</th>
                      <th>Appointment Date</th>
                  </tr>
                </thead>
                <tbody>
                    <% data.forEach(data => { %>
                        <tr>
                            <td><%-data.patientname %></td>
                            <td>
                              <img src="/<%-data.image %>" alt="" style="height: 60px;width: 60px;">
                                </td>
                            <td><%-data.symptom %></td>
                            <td><%-data.contact %></td>
                            <td><%-data.address%></td>
                            <td><%-data.date%></td>
                        </tr>
                        <% }) %>
                </tbody>
              </table>
        </div>
    </div>
</div>