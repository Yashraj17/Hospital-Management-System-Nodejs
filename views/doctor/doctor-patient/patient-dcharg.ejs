<%- include('../navbar') %>
<div class="container-fluid ps-0">
    <div class="row w-100 p-0">
        <div class="col-2" id="side" style="height:91vh;">
            <%- include('../side') %>
        </div>
        <div class="col-10">
            <h4 class="mt-3 border-start border-4 px-2">Your Discharged Patient List</h4>
            <table class="table mt-4">
                <thead class="table-dark">
                  <tr>
                      <th>Patient Name</th>
                      <th>Admit Date</th>
                      <th>Release Date</th>
                      <th>Mobile</th>
                      <th>Symptoms</th>
                      <th>Address</th>
                  </tr>
                </thead>
                <tbody>
                    <% data.forEach(data => { %>
                        <tr>
                            <td><%-data.patientname %></td>
                            <td><%-data.admitDate %></td>
                            <td><%-data.releaseDate %></td>
                            <td><%-data.patientId.contact %></td>
                            <td><%-data.patientId.symptom %></td>
                            <td><%-data.patientId.address %></td>
                        </tr>
                        <% }) %>
                </tbody>
              </table>
        </div>
    </div>
</div>