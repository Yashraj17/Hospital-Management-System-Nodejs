<%- include('../navbar') %>
<div class="container-fluid ps-0">
    <div class="row w-100 p-0">
        <div class="col-2" id="side" style="height:91vh;">
            <%- include('../side') %>
        </div>
        <div class="col-10">
            <h4 class="mt-3 border-start border-4 px-2">Discharge Patients</h4>
            <table class="table mt-4">
                <thead class="table-dark">
                  <tr>
                      <th>Name</th>
                      <th>Profile Picture</th>
                      <th>Contact</th>
                      <th>Symptoms</th>
                      <th>Admit Date</th>
                      <th>Discharge</th>
                  </tr>
                </thead>
                <tbody>
                    <% data.forEach(data => { %>
                        <tr>
                            <td><%-data.patientname %></td>
                            <td>
                              <img src="/<%-data.image %>" alt="" style="height: 60px;width: 60px;">
                            </td>
                            <td><%-data.contact %></td>
                            <td><%-data.symptom %></td>
                            <td><%-data.date%></td>
                            <td>
                                <a href="/admin/discharge-patient/<%-data._id %>" class="btn btn-sm btn-success ms-3" ><i class="bi bi-box-arrow-in-right"></i></a>
                            </td>
                        </tr>
                        <% }) %>
                </tbody>
              </table>
        </div>
    </div>
</div>